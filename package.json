{
  "name": "ekaty",
  "version": "0.1.0",
  "private": true,
  "description": "Local restaurant discovery & engagement platform for Katy, Texas",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start -p ${PORT:-8080}",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "seed": "ts-node prisma/seed-katy.ts",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:seed-google": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma/seed-google-places.ts",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
    "db:reset": "prisma migrate reset --force",
    "import:google": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/import-google-places.ts",
    "import:test": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/test-google-api.ts",
    "sync-restaurants": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/sync-restaurants.ts",
    "update:all-listings": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/update-all-listings.ts",
    "generate:blog-seed": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/generate-blog-seed-articles.ts",
    "update:grub-roulette-article": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/update-grub-roulette-article.ts",
    "unfeature-all": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/unfeature-all-restaurants.ts",
    "generate:launch-article": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/generate-launch-article-direct.ts",
    "generate:batch-articles": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/generate-launch-articles-batch.ts",
    "generate:single-article": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/generate-single-article.ts",
    "continue:articles": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/continue-article-generation.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@aws-sdk/client-s3": "^3.932.0",
    "@flydotio/litestream": "^1.0.1",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@prisma/client": "^5.22.0",
    "@react-email/components": "^1.0.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@types/leaflet": "^1.9.21",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "csv-parse": "^5.5.3",
    "dotenv": "^17.2.3",
    "html-to-image": "^1.11.13",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.554.0",
    "next": "^14.2.33",
    "next-auth": "^4.24.13",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "p-limit": "^7.1.1",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^5.0.4",
    "react-hook-form": "^7.48.2",
    "react-leaflet": "^4.2.1",
    "resend": "^6.3.0",
    "stripe": "^17.7.0",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcrypt": "^5.0.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "jest": "^30.2.0",
    "postcss": "^8.4.32",
    "prisma": "^5.22.0",
    "puppeteer": "^24.23.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dockerfile": {
    "litestream": true
  }
}
